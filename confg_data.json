{
    "Cnaes": ["id TEXT NOT NULL PRIMARY KEY", "descricao TEXT"],
    "Motivos": ["id TEXT NOT NULL PRIMARY KEY", "descricao TEXT"],
    "Municipios": ["id TEXT NOT NULL PRIMARY KEY", "descricao TEXT"],
    "Naturezas": ["id TEXT NOT NULL PRIMARY KEY", "descricao TEXT"],
    "Paises": ["id TEXT NOT NULL PRIMARY KEY", "descricao TEXT"],
    "Qualificacoes": ["id TEXT NOT NULL PRIMARY KEY", "descricao TEXT"],
    "Simples": [
        "cnpj_basico TEXT",
        "opcao_pelo_simples TEXT",
        "data_opcao_pelo_simples TEXT",
        "data_exclusao_do_simples TEXT",
        "opcao_pelo_mei TEXT",
        "data_opcao_pelo_mei TEXT",
        "data_exclusao_do_mei TEXT",
        "FOREIGN KEY(cnpj_basico) REFERENCES Empresas(cnpj_basico)"
    ],
    "Socios": [
        "cnpj_basico TEXT",
        "identificador_socio TEXT",
        "nome_socio TEXT",
        "cnpj_cpf_socio TEXT",
        "qualificacao_socio TEXT",
        "data_entrada_sociedade TEXT",
        "pais TEXT",
        "representante_legal TEXT",
        "nome_representante TEXT",
        "qualificacao_representante_legal TEXT",
        "faixa_etaria TEXT",
        "FOREIGN KEY(cnpj_basico) REFERENCES Empresas(cnpj_basico)",
        "FOREIGN KEY(pais) REFERENCES Paises(id)",
        "FOREIGN KEY(qualificacao_socio) REFERENCES Qualificacoes(id)"
    ],
    "Empresas": [
        "cnpj_basico TEXT NOT NULL PRIMARY KEY",
        "razao_social TEXT",
        "natureza_juridica TEXT",
        "qualificacao_responsavel TEXT",
        "capital_social_str TEXT",
        "porte_empresa TEXT",
        "ente_federativo_responsavel TEXT",
        "FOREIGN KEY(natureza_juridica) REFERENCES Naturezas(id)"
    ],
    "Estabelecimentos": [
        "cnpj_basico TEXT",
        "cnpj_ordem TEXT",
        "cnpj_dv TEXT",
        "matriz_filial TEXT", 
        "nome_fantasia TEXT",
        "situacao_cadastral TEXT",
        "data_situacao_cadastral TEXT", 
        "motivo_situacao_cadastral TEXT",
        "nome_cidade_exterior TEXT",
        "pais TEXT",
        "data_inicio_atividades TEXT",
        "cnae_fiscal TEXT",
        "cnae_fiscal_secundaria TEXT",
        "tipo_logradouro TEXT",
        "logradouro TEXT", 
        "numero TEXT",
        "complemento TEXT",
        "bairro TEXT",
        "cep TEXT",
        "uf TEXT",
        "municipio TEXT",
        "ddd1 TEXT",
        "telefone1 TEXT",
        "ddd2 TEXT",
        "telefone2 TEXT",
        "ddd_fax TEXT",
        "fax TEXT",
        "correio_eletronico TEXT",
        "situacao_especial TEXT",
        "data_situacao_especial TEXT",
        "FOREIGN KEY(cnpj_basico) REFERENCES Empresas(cnpj_basico)",
        "FOREIGN KEY(pais) REFERENCES Paises(id)",
        "FOREIGN KEY(municipio) REFERENCES Municipios(id)",
        "FOREIGN KEY(cnae_fiscal) REFERENCES Cnaes(id)"
    ]
}
